{
  "name": "@openbug/cli",
  "version": "0.1.4",
  "type": "module",
  "bin": {
    "debug": "bin/openbug.js"
  },
  "files": [
    "bin/",
    "dist/",
    "patches/",
    "studio/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "tsx index.tsx",
    "dev": "tsx index.tsx",
    "code-hierarchy": "tsx code_hierarchy.ts",
    "build": "tsc && npm run copy-js && npm run clean-maps",
    "copy-js": "mkdir -p dist/helpers && cp helpers/config-helpers.js dist/helpers/config-helpers.js",
    "clean-maps": "find dist -name '*.map' -type f -delete 2>/dev/null || true",
    "prepublishOnly": "patch-package && npm run build",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "ai": "^4.0.0",
    "@types/ws": "^8.18.1",
    "@vscode/ripgrep": "^1.17.0",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "ignore": "^5.3.2",
    "ink": "^6.6.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "js-yaml": "^4.1.1",
    "langchain": "1.2.3",
    "marked": "^15.0.0",
    "marked-terminal": "^7.3.0",
    "node-pty": "0.10.1",
    "patch-package": "^8.0.1",
    "react": "^19.2.0",
    "strip-ansi": "^7.1.2",
    "ws": "^8.18.3",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/react": "^19.1.13",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3"
  }
}
